<ul>
    {{#each this}}
    {{#if scene}}
    <div class="col col-3 card text-center" style="margin: 0.5%">
        <div class="card-body">
            <p class="card-text"><i>{{scene}}_{{zone}}#{{id}}</i></p>
        </div>
        <div class="card-footer text-muted">
            {{!-- Created:&#09;{{createdAt}} --}}
            <br />
            {{!-- Updated:&#09;{{updatedAt}} --}}
        </div>
    </div>
    {{/if}}
    {{/each}}
</ul>